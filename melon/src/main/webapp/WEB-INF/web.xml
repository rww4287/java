<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>melon</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <display-name>CharacterEncodingFilter</display-name>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.melon.common.web.CharacterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>ViewArtistListServlet</display-name>
    <servlet-name>ViewArtistListServlet</servlet-name>
    <servlet-class>com.melon.artist.web.ViewArtistListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewArtistListServlet</servlet-name>
    <url-pattern>/artist/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewArtistWriteServlet</display-name>
    <servlet-name>ViewArtistWriteServlet</servlet-name>
    <servlet-class>com.melon.artist.web.ViewArtistWriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewArtistWriteServlet</servlet-name>
    <url-pattern>/artist/write</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewAlbumListServlet</display-name>
    <servlet-name>ViewAlbumListServlet</servlet-name>
    <servlet-class>com.melon.album.web.ViewAlbumListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAlbumListServlet</servlet-name>
    <url-pattern>/album/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewAlbumWriteServlet</display-name>
    <servlet-name>ViewAlbumWriteServlet</servlet-name>
    <servlet-class>com.melon.album.web.ViewAlbumWriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAlbumWriteServlet</servlet-name>
    <url-pattern>/album/write</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewAlbumPostServlet</display-name>
    <servlet-name>ViewAlbumPostServlet</servlet-name>
    <servlet-class>com.melon.album.web.ViewAlbumPostServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAlbumPostServlet</servlet-name>
    <url-pattern>/album/post</url-pattern>
  </servlet-mapping>
</web-app>